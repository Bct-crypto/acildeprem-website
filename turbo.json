{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".dist/**"
            ]
        },
        "dev": {
            "dependsOn": [
                "^dev"
            ],
            "cache": false
        }
    }
}